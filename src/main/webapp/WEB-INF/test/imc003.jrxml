    <queryString>
		<![CDATA[SELECT
     (b.c_code+' '+b.c_name) AS c_name,
     sum(d.f_total_cost) AS f_total_cost,
     sum(d.f_disc_com_amt) AS f_disc_com_amt,
     sum(d.f_disc_cash_amt) AS f_disc_cash_amt,
     a."ar_so_hdr_id",
     a."dc_cnt_id",
     a."c_code",
     a."i_is_sale_external",
     a."cnt_emp_id",
     a."dc_comm_id",
     a."d_so_date",
     a."c_so_no",
     a."c_po_no",
     a."c_comment",
     a."c_advertising",
     a."dc_cost_id" AS dc_cost_id,
     d."dc_product_type_id" AS dc_product_type_id,
     b."dc_cnt_type_id" AS dc_cnt_type_id,
(select c_name from dc_cnt_type where dc_cnt_type_id=b."dc_cnt_type_id") as dc_cnt_type_name,
(select c_name from dc_product_type where dc_product_type_id=d."dc_product_type_id") as product_type_name,
(select c_name from dc_cost where dc_cost_id=a."dc_cost_id") as dc_cost_name
FROM "ar_so_hdr" a
     INNER JOIN "vw_ar_so_dtl_tv" d ON d."ar_so_hdr_id" = a."ar_so_hdr_id" and d."i_enabled" = 1
     INNER JOIN "dbo"."vw_customer" b ON a."dc_cnt_id" = b."dc_cnt_id"
WHERE
     a.c_code! = '0'
     and a.i_enabled = 1
     and a.i_class_type = 1
     and a.onair_yyyy_mm = $P{c_yyyy_mm}
     and a.i_is_commit = 1
     and ISNULL(a.dc_comm_id,0)! = 0
GROUP BY
     a.ar_so_hdr_id,
     a.dc_cnt_id,
     a.c_code,
     a.i_is_sale_external,
     a.cnt_emp_id,
     a.dc_comm_id,
     a.d_so_date,
     a.c_so_no,
     a.c_po_no,
     a.c_comment,
     a.c_advertising,
     b.c_code,
     b.c_name,
     a.dc_cost_id,
     d.dc_product_type_id,
     b.dc_cnt_type_id
ORDER BY
     d.dc_product_type_id ASC,
     a.dc_cost_id ASC,
     b.dc_cnt_type_id ASC,
     b.c_code ASC,
     a.c_code ASC]]>
    </queryString>
