    <queryString>
		<![CDATA[SELECT
     pj_hdr."c_code" AS pj_hdr_c_code,
     pj_hdr."c_name" AS pj_hdr_c_name,
     pj_hdr."dc_cost_id" AS pj_hdr_dc_cost_id,
     (select top 1 c_name from dc_cost where dc_cost_id=pj_hdr."dc_cost_id") as dc_cost_name,
     (select top 1 c_name from dc_cnt where dc_cnt_id=pj_hdr."dc_cnt_id") as dc_cnt_name,
     pj_hdr."dc_cnt_id" AS pj_hdr_dc_cnt_id,
     pj_hdr."c_contract_no" AS pj_hdr_c_contract_no,
     pj_hdr."c_doc_bespeak" AS pj_hdr_c_doc_bespeak,
     convert(varchar, pj_hdr."d_start", 120) AS pj_hdr_d_start,
     convert(varchar, pj_hdr."d_finish", 120) AS pj_hdr_d_finish,
     pj_hdr."f_budget" AS pj_hdr_f_budget,
     pj_hdr."i_is_success" AS pj_hdr_i_is_success,
CASE
    WHEN pj_hdr."i_is_success" = 1 THEN 'ปิด'
    ELSE 'เปิด'
END AS status_name

FROM
     "dbo"."pj_hdr" pj_hdr
WHERE pj_hdr."c_code" !='0'
and dbo.get_stryyyy_mm(pj_hdr."d_start") between $P{c_yyyy_mm} and $P{c_yyyy_mm2}]]>
    </queryString>
